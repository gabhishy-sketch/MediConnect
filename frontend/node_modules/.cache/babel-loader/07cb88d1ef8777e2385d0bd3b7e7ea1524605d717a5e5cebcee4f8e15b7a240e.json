User(userObj || null);\r\n    navigate('/');\r\n  };\r\n\r\n  const register = (tokenValue, userObj) => {\r\n    setToken(tokenValue);\r\n    setUser(userObj || null);\r\n    navigate('/');\r\n  };\r\n\r\n  const logout = () => {\r\n    setToken(null);\r\n    setUser(null);\r\n    navigate('/login');\r\n  };\r\n\r\n  return (\r\n    <AuthContext.Provider value={{ user, token, setToken, setUser, login, register, logout }}>\r\n      {children}\r\n    </AuthContext.Provider>\r\n  );\r\n}"],"mappings":"AAAA;AACA,MAAO,CAAAA,KAAK,EAAIC,aAAa,CAAEC,QAAQ,CAAEC,SAAS,KAAQ,OAAO,CACjE,OAASC,WAAW,KAAQ,kBAAkB,CAAC,OAAAC,GAAA,IAAAC,IAAA,yBAE/C,MAAO,MAAM,CAAAC,WAAW,cAAGN,aAAa,CAAC,CAAC,CAE1C,MAAO,SAAS,CAAAO,YAAYA,CAAAC,IAAA,CAAe,IAAd,CAAEC,QAAS,CAAC,CAAAD,IAAA,CACvC,KAAM,CAAAE,QAAQ,CAAGP,WAAW,CAAC,CAAC,CAC9B,KAAM,CAACQ,IAAI,CAAEC,OAAO,CAAC,CAAGX,QAAQ,CAAC,IAAI,CAAC,CACtC,KAAM,CAACY,KAAK,CAAEC,QAAQ,CAAC,CAAGb,QAAQ,CAAC,IAAMc,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,EAAI,IAAI,CAAC,CAE/Ed,SAAS,CAAC,IAAM,CACd,GAAIW,KAAK,CAAEE,YAAY,CAACE,OAAO,CAAC,OAAO,CAAEJ,KAAK,CAAC,CAAC,IAC3C,CAAAE,YAAY,CAACG,UAAU,CAAC,OAAO,CAAC,CACvC,CAAC,CAAE,CAACL,KAAK,CAAC,CAAC,CAEX,KAAM,CAAAM,KAAK,CAAGA,CAACC,UAAU,CAAEC,OAAO,GAAK,CACrCP,QAAQ,CAACM,UAAU,CAAC,CACpBR,OAAO,CAACS,OAAO,EAAI,IAAI,CAAC,CACxBX,QAAQ,CAAC,GAAG,CAAC,CACf,CAAC,CAED,KAAM,CAAAY,QAAQ,CAAGA,CAACF,UAAU,CAAEC,OAAO,GAAK,CACxCP,QAAQ,CAACM,UAAU,CAAC,CACpBR,OAAO,CAACS,OAAO,EAAI,IAAI,CAAC,CACxBX,QAAQ,CAAC,GAAG,CAAC,CACf,CAAC,CAED,KAAM,CAAAa,MAAM,CAAGA,CAAA,GAAM,CACnBT,QAAQ,CAAC,IAAI,CAAC,CACdF,OAAO,CAAC,IAAI,CAAC,CACbF,QAAQ,CAAC,QAAQ,CAAC,CACpB,CAAC,CAED,mBACEL,IAAA,CAACC,WAAW,CAACkB,QAAQ,EAACC,KAAK,CAAE,CAAEd,IAAI,CAAEE,KAAK,CAAEC,QAAQ,CAAEF,OAAO,CAAEO,KAAK,CAAEG,QAAQ,CAAEC,MAAO,CAAE,CAAAd,QAAA,CACtFA,QAAQ,CACW,CAAC,CAE3B","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}